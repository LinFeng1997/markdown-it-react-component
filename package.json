    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-react": "^7.18.3",
    "highlight.js": "^9.14.2",
    "jest": "^24.9.0",
    "markdown-it": "^8.4.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.2.1",
    "rollup-plugin-babel": "^4.3.2",
    "tui-editor": "^1.3.0"
